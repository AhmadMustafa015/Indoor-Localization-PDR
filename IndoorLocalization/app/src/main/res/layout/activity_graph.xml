<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".InertiaNavegation.activity.GraphActivity"
    android:id="@+id/graph_layout_relative">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayoutGraph">

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
        <TextView
            android:id="@+id/TV_totalDistace"
            android:layout_width="0dp"
            android:layout_height="57dp"
            android:layout_weight="1"
            android:text="0"
            android:layout_alignParentTop="true"
            android:textAlignment="center"
            android:textIsSelectable="false"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/TV_numSteps"
            android:layout_width="wrap_content"
            android:layout_height="59dp"
            android:layout_weight="1"
            android:text="0"
            android:layout_alignParentTop="true"
            android:textAlignment="center"
            android:textSize="30sp" />
    </LinearLayout>


    <com.google.android.material.floatingactionbutton.FloatingActionButton

    android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:tint="@android:color/white"
        android:backgroundTint="#ff0099ff"
        app:borderWidth="0dp"
        app:fabSize="normal"
        app:srcCompat="@drawable/ic_play_arrow_black_24dp" />

    <SeekBar
        android:id="@+id/seekbar_speed"
        android:layout_width="288dp"
        android:layout_height="79dp"
        android:layout_alignStart="@+id/linearLayoutGraph"
        android:layout_alignEnd="@+id/linearLayoutGraph"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="645dp"
        android:layout_marginEnd="82dp"
        android:layout_marginBottom="-9dp"
        android:defaultValue="true"
        android:key="walking_seeed"
        android:max="10"
        android:min="2"
        android:summary="3" />

</RelativeLayout>

